{
  "manifest_version": 2,
  "version": "1.1",
  "name": "dark-pixels",
  "theme": {
    "images": {
      "additional_backgrounds": [
        "images/background.jpg"
      ]
    },
    "properties": {
      "additional_backgrounds_alignment": [
        "center center"
      ],
      "additional_backgrounds_tiling": [
        "repeat"
      ]
    },
    "colors": {
      "toolbar": "rgba(16, 16, 16, 0.85)",
      "toolbar_text": "rgb(255, 255, 255)",
      "frame": "rgb(8, 8, 8)",
      "tab_background_text": "rgb(255, 255, 255)",
      "toolbar_field": "rgba(32, 32, 32, 0.85)",
      "toolbar_field_text": "rgb(255, 255, 255)",
      "tab_line": "rgb(255, 255, 255)",
      "popup": "rgba(16, 16, 16, 0.9)",
      "popup_text": "rgb(255, 255, 255)",
      "button_background_active": "rgba(48, 48, 48, 0.9)",
      "button_background_hover": "rgba(32, 32, 32, 0.9)",
      "ntp_background": "rgb(16, 16, 16)",
      "sidebar_highlight": "rgba(32, 32, 32, 0.9)",
      "toolbar_bottom_separator": "rgb(128, 128, 128)",
      "toolbar_top_separator": "rgb(96, 96, 96)",
      "toolbar_vertical_separator": "rgb(96, 96, 96)",
      "tab_loading": "rgb(255, 255, 255)"
    }
  }
}
